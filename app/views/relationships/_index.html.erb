<table class="table">
        <thead>
          <tr>
            <th></th>
            <th>ユーザー名</th>
            <th>紹介文</th>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% users.each do |user| %>
          <tr>
            <td><%= image_tag user.get_profile_image, size:"50x50" %></td>
            <td><%= user.name %></td>
            <td><%= user.introduction %></td>
            <td>フォロー数:<%= user.followings.count %></td>
            <td>フォロワー数:<%= user.followers.count %></td>
            <td><%= render 'btn', user: user %></td>
            <td><%= link_to "Show", user_path(user) %></td>
          </tr>
          <% end %>
        </tbody>
      </table>